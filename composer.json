{
    "name": "ionutsidenis/actorawardsvisualizer",
    "description": "Actor Awards Visualizer - Explore and analyze SAG Awards nominations and wins",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "ext-pdo_sqlite": "*",
        "ext-simplexml": "*",
        "intervention/image": "^2.7",
        "phenx/php-svg-lib": "^0.5.0",
        "phpmailer/phpmailer": "^6.10",
        "rosell-dk/webp-convert": "^2.9",
        "vlucas/phpdotenv": "^5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "ActorAwards\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ActorAwards\\Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit",
        "dump-autoload": "composer dump-autoload"
    }
}
